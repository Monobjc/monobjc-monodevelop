<?xml version="1.0" encoding="utf-8"?>
<Addin id="MonoDevelop.Monobjc.Refactoring"
       name="Monobjc Refactoring"
       author="Laurent Etiemble"
       url="http://www.monobjc.net/"
       description="Monobjc Refactoring"
       copyright="GPL"
       category="IDE extensions"
       version="2.8.9999.0">
	
  <Runtime>
    <!-- Assemblies -->
    <Import assembly="MonoDevelop.Monobjc.Refactoring.dll" />
  </Runtime>

  <!-- Dependencies -->
  <Dependencies>
    <Addin id="MonoDevelop.Core" version="2.8"/>
    <Addin id="MonoDevelop.Ide" version="2.8"/>
    <Addin id="MonoDevelop.Refactoring" version="2.8"/>
    <Addin id="MonoDevelop.Monobjc" version="2.8"/>
  </Dependencies>

  <Extension path="/MonoDevelop/Refactoring/Refactorings">
    <Class id="CreateObjectiveCIVar" class="MonoDevelop.Monobjc.Refactoring.CreateIVarOperation" />
    <Class id="ImplementObjectiveCProtocol" class="MonoDevelop.Monobjc.Refactoring.ImplementProtocolOperation" />
  </Extension>

  <Extension path="/MonoDevelop/Ide/Commands/Refactoring">
    <Command id="MonoDevelop.Monobjc.Refactoring.CreateObjectiveCIVar"
             defaultHandler="MonoDevelop.Monobjc.Refactoring.CreateIVarHandler"
             _label="Create Variable"/>
    <Command id="MonoDevelop.Monobjc.Refactoring.ImplementObjectiveCProtocol"
             defaultHandler="MonoDevelop.Monobjc.Refactoring.ImplementProtocolHandler"
             _label="Implement Protocol"/>
  </Extension>

    <Header>
		<Icon32>Icons/monobjc-logo-32.png</Icon32>
	</Header>

  
</Addin>